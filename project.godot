; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="TextLife"
run/main_scene="res://Scenes/SplashScreen.tscn"
config/features=PackedStringArray("4.0", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

CharacterDatabase="*res://Scripts/Character/CharacterDatabase.cs"
CountryDatabase="*res://Scripts/Country/CountryDatabase.cs"
GameController="*res://Scripts/Controllers/GameController.cs"
CharacterGenerator="*res://Scripts/Character/CharacterGenerator.cs"
EventDatabase="*res://Scripts/MainGameView/EventSystem/EventDatabase.cs"
SaveGameController="*res://Scripts/Save System/SaveGameController.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3

[dotnet]

project/assembly_name="TextLife"

[rendering]

renderer/rendering_method="mobile"
